<!doctype html>
<html>
  <head>
    <meta charset="utf-8">

    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">

    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title %></title>

    <%= stylesheet_link_tag "all" %>
    <%= javascript_include_tag  "all" %>
    <%= javascript_include_tag  "highlight/highlight.pack" %>
    <script>hljs.initHighlightingOnLoad();</script>
  </head>


  <body class="<%= page_classes %>">

    <div class="row">
        <div class="small-12 columns">
          <a href="http://mattlima.github.io/mooog/"><span class="supertitle label radius">Mooog examples</span></a>
              <h1><%= locals[:ordinal] %>. <%= locals[:name] %></h1>
        </div>
    </div>
    <%= yield %>

     <%= partial 'partials/footer' %>


     <%= javascript_include_tag "body-end" %>

  </body>
</html>
